<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.WorksheetController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <Label layoutX="37.0" layoutY="14.0" text="Manage Worksheet">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
            <Button fx:id="saveButton" layoutX="49.0" layoutY="342.0" mnemonicParsing="false" onAction="#save" text="Save" />
            <Button fx:id="cancelButton" layoutX="152.0" layoutY="341.0" mnemonicParsing="false" onAction="#cancel" text="Cancel" />
            <Button fx:id="addTestButton" layoutX="93.0" layoutY="303.0" mnemonicParsing="false" onAction="#addTest" text="Add tests" />
            <Button fx:id="smallWeekButton" layoutX="95.0" layoutY="41.0" mnemonicParsing="false" onAction="#updateTableSmall" text="Small" />
            <Label layoutX="12.0" layoutY="45.0" text="Week Size" />
            <Button fx:id="largeWeekButton" layoutX="183.0" layoutY="41.0" mnemonicParsing="false" onAction="#updateTableLarge" text="Large" />
            <ScrollPane layoutX="-1.0" layoutY="89.0" prefHeight="214.0" prefWidth="254.0" vbarPolicy="NEVER">
              <content>
                <AnchorPane />
              </content>
            </ScrollPane>
            <Label layoutX="22.0" layoutY="70.0" prefHeight="18.0" prefWidth="54.0" text="Test" textAlignment="CENTER" />
            <Label layoutX="181.0" layoutY="70.0" text="Amount" />
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
    </AnchorPane>
  </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</VBox>
